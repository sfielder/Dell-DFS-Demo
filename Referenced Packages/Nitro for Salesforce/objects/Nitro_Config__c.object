    <validationRules>
        <fullName>Points_Name_Long_Validation</fullName>
        <active>true</active>
        <description>Don&apos;t allow single or double quotes in Points Name (Long).</description>
        <errorConditionFormula>OR (CONTAINS( Points_Name_long__c , &quot;&amp;quot;&quot;),   CONTAINS(Points_Name_long__c , &quot;&amp;#39;&quot;), CONTAINS(Points_Name_long__c , &quot;\&quot;&quot;), CONTAINS(Points_Name_long__c , &quot;&apos;&quot;), CONTAINS(Points_Name_long__c , &quot;&amp;apos;&quot;))</errorConditionFormula>
        <errorDisplayField>Points_Name_long__c</errorDisplayField>
        <errorMessage>Quotes and Apostrophes aren&apos;t allowed in the Long Point Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Points_Name_Short_Validation</fullName>
        <active>true</active>
        <description>Don&apos;t allow single or double quotes in Points Name (Short).</description>
        <errorConditionFormula>OR (CONTAINS( Points_Name_short__c, &quot;&amp;quot;&quot;),   CONTAINS(Points_Name_short__c, &quot;&amp;#39;&quot;), CONTAINS(Points_Name_short__c, &quot;\&quot;&quot;), CONTAINS(Points_Name_short__c, &quot;&apos;&quot;), CONTAINS(Points_Name_short__c, &quot;&amp;apos;&quot;))</errorConditionFormula>
        <errorDisplayField>Points_Name_short__c</errorDisplayField>
        <errorMessage>Quotes and Apostrophes aren&apos;t allowed in the Short Point Name.</errorMessage>
    </validationRules>
