        <externalId>false</externalId>
        <formula>IF(ISBLANK(ProfileUrl__c), IMAGE(AvatarIconResource__c, UserName__c, 48, 48), HYPERLINK(ProfileUrl__c ,IMAGE(AvatarIconResource__c, UserName__c, 48, 48)))</formula>
