    <webLinks>
        <fullName>Revoke_Facebook_Access</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Revoke Facebook Access</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 

if({!Fcbk_User__c.Valid__c}){
        
   var agree=confirm(&quot;{!$Label.REVOKE_FACEBOOK_USER_ACCESS_CONFIRM}&quot;);

   if (agree){

		window.location = &quot;/apex/sf4twitter__FcbkOAuth?revoke=1&amp;type=user&amp;accId={!Fcbk_User__c.Id}&amp;retURL=&quot; + encodeURIComponent(window.location);


   }
}else{
   alert(&apos;{!JSENCODE($Label.REVOKE_USER_NOT_VALIDATED)}&apos;);
}</url>
    </webLinks>
