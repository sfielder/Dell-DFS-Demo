        <externalId>false</externalId>
        <formula>HYPERLINK( &apos;http://www.facebook.com/pages/&apos;  + Name + &apos;/&apos; +  Fcbk_Id__c ,
IF( Image__c  &lt;&gt;null, IMAGE( Image__c , &quot;&quot;), null )
,&quot;&quot;)</formula>
